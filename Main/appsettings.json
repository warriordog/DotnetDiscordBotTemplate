{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "DSharpPlus": "Information",
            "System": "Information",
            "Microsoft": "Information",
            "DiscordBotTemplate": "Information"
        },
        "Console": {
            "FormatterName": "simple",
            "FormatterOptions": {
                "SingleLine": true,
                "IncludeScopes": true,
                "TimestampFormat": "[yyyy-MM-dd HH:mm:ss.fff K] "
            }
        }
    },
    "BotOptions": {
        "CommandPrefixes": [ "bot!" ]
    }
}